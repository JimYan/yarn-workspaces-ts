{
    "name": "@quramy/x-a",
    "version": "1.0.0",
    "description": "",
    "keywords": [],
    "repository": {
        "type": "git",
        "url": ""
    },
    "main": "dist/index.js",
    "license": "MIT",
    "engines": {
        "node": ">=6.0.0"
    },
    "types": "dist/index.d.ts",
    "scripts": {
        "test": "jest --coverage"
    },
    "jest": {
        "transform": {
            ".(ts|tsx)": "ts-jest"
        },
        "testEnvironment": "node",
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "/test/"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 90,
                "functions": 95,
                "lines": 95,
                "statements": 95
            }
        },
        "collectCoverageFrom": [
            "src/*.{js,ts}"
        ]
    },
    "devDependencies": {}
}
